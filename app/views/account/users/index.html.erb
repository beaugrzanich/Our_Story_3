<%= render 'account/shared/page' do |p| %>
    <% p.content_for :title, "User List" %>
    <% p.content_for :body do %>
        <ul class="users">
        <% @users.each do |user| %>
        <li>
        <%= gravatar_for user, size: 50 %>
         <%= link_to user.first_name, account_user_path(user.id) %>
         <%= link_to user.last_name, account_user_path(user.id) %>
        </li> <% end %>
        </ul>
    <% end %>
  <% end %>